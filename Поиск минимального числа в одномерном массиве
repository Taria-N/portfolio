public class arrMin {
    public static void main(String[] args) throws IOException {

        int[] list = {1, 5, 12, -9, 3, -12};
        int min = list[0];


        for (int i = 0; i < list.length; i++) {
            if (list[i] < min) {
                min = list[i];
            }


        }
        System.out.println("Семое минимальное число - " + min);
    }
}
