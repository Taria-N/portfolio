public class arrSum {
    public static void main(String[] args) throws IOException {

        int[] list = {1, 5, 12, 3, -2};
        int sum = 0;


        for (int i = 0; i < list.length; i++) {
            sum = sum + list[i];

        }
        System.out.println("Сумма чисел массива - " + sum);
    }
}
